{{${P}each securityDefinitions}}
${P}${P}${P} {{@key}}
{{${P}this}}
{{${P}ifeq type "oauth2"}}
<table>
    <tr>
        <th>type</th>
        <th colspan="2">{{type}}</th>
    </tr>
{{${P}if description}}
        <tr>
            <th>description</th>
            <th colspan="2">{{description}}</th>
        </tr>
{{/if}}
{{${P}if authorizationUrl}}
        <tr>
            <th>authorizationUrl</th>
            <th colspan="2">{{authorizationUrl}}</th>
        </tr>
{{/if}}
{{${P}if flow}}
        <tr>
            <th>flow</th>
            <th colspan="2">{{flow}}</th>
        </tr>
{{/if}}
{{${P}if tokenUrl}}
        <tr>
            <th>tokenUrl</th>
            <th colspan="2">{{tokenUrl}}</th>
        </tr>
{{/if}}
{{${P}if scopes}}
    <tr>
        <td rowspan="3">scopes</td>
{{${P}each scopes}}
            <td>{{@key}}</td>
            <td>{{this}}</td>
        </tr>
        <tr>
{{/each}}
    </tr>
{{/if}}
</table>
{{/ifeq}}
{{${P}ifeq type "apiKey"}}
<table>
    <tr>
        <th>type</th>
        <th colspan="2">{{type}}</th>
    </tr>
{{${P}if description}}
        <tr>
            <th>description</th>
            <th colspan="2">{{description}}</th>
        </tr>
{{/if}}
{{${P}if name}}
        <tr>
            <th>name</th>
            <th colspan="2">{{name}}</th>
        </tr>
{{/if}}
{{${P}if in}}
        <tr>
            <th>in</th>
            <th colspan="2">{{in}}</th>
        </tr>
{{/if}}
</table>
{{/ifeq}}
{{${P}ifeq type "basic"}}
<table>
    <tr>
        <th>type</th>
        <th colspan="2">{{type}}</th>
    </tr>
{{${P}if description}}
        <tr>
            <th>description</th>
            <th colspan="2">{{description}}</th>
        </tr>
{{/if}}
</table>
{{/ifeq}}
{{/this}}
{{/each}}